@page
@model WisDellsDevApp.Pages.AgenciesModel
@{
}
<br />
<h2>Agencies</h2>

<table border="1">
    <thead>
        <tr>
            <th>RecNo</th>
            <th>Name</th>
            <th>Name2</th>
            <th>Name3</th>
       </tr>
    </thead>
    <tbody>
        @foreach (var agency in Model.listAgencies)
        {
         <tr>
            <td>@agency.RecNo</td>
            <td>@agency.DeliveryType</td>
            <td>@agency.DeliveredWhen</td>
            <td>@agency.PaymentAmount</td>
        </tr>
        }
    </tbody>
</table>
